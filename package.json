{
  "name": "gentool",
  "version": "0.1.0",
  "description": "tool for easy starter of common ts-node-express app",
  "main": "server/index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "dev:node": "ts-node src/index.ts",
    "dev:demon": "nodemon --config nodemon.json",
    "build": "npm run lint && tsc src/index.ts",
    "start": "node src",
    "prod": "npm run build && npm start"
  },
  "keywords": [
    "Gentool",
    "Starter"
  ],
  "author": "Danyil Moroz",
  "license": "MIT",
  "dependencies": {
    "@types/clui": "^0.3.0",
    "@types/configstore": "^4.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.0.3",
    "@types/lodash": "^4.14.135",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.0.10",
    "@types/redis": "^2.8.13",
    "@types/touch": "^3.1.1",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "configstore": "^5.0.0",
    "dotenv": "^8.0.0",
    "figlet": "^1.2.3",
    "inquirer": "^6.4.1",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "redis": "^2.8.0",
    "touch": "^3.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "tslint": "^5.18.0"
  }
}
